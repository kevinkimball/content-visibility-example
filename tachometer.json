{
  "$schema": "https://raw.githubusercontent.com/Polymer/tachometer/master/config.schema.json",
  "sampleSize": 50,
  "benchmarks": [
    {
      "url": "http://localhost:3001/homepage.html",
      "browser": {
        "name": "chrome",
        "addArguments": ["use-mobile-user-agent"],
        "windowSize": {
          "width": 360,
          "height": 640
        }
      },
      "measurement": "global"
    },
    {
      "url": "http://localhost:3001/blank.html",
      "browser": {
        "name": "chrome",
        "addArguments": ["use-mobile-user-agent"],
        "windowSize": {
          "width": 360,
          "height": 640
        }
      },
      "measurement": "global"
    }
  ]
}
